{
  "name": "weave-io",
  "version": "0.1.4",
  "description": "A framework for building universal inferno applications",
  "main": "./build/index.js",
  "scripts": {
    "build": "babel core --out-dir dist",
    "prepublish": "npm run build"
  },
  "bin": {
    "weave": "./bin/weave"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samuelngs/weave.git"
  },
  "keywords": [
    "inferno",
    "universal",
    "framework",
    "applications"
  ],
  "dependencies": {
    "inferno": "^0.7.27",
    "inferno-dom": "^0.7.27",
    "inferno-server": "^0.7.27",
    "inferno-router": "^0.7.27",
    "inferno-component": "^0.7.27",
    "inferno-create-element": "^0.7.27",
    "inferno-redux": "^0.7.27",
    "redux": "^3.6.0",
    "redux-persist": "^4.0.0-alpha7",
    "localforage": "^1.4.3",
    "isomorphic-fetch": "^2.2.1",
    "express": "^4.14.0",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "ua-parser-js": "0.7.12",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.7",
    "babel-generator": "6.17.0",
    "babel-cli": "^6.18.0",
    "babel-plugin-inferno": "^1.0.0-legacy",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-async-to-generator": "6.16.0",
    "babel-plugin-transform-class-properties": "6.16.0",
    "babel-plugin-transform-object-rest-spread": "6.16.0",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-plugin-module-resolver": "^2.3.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "extract-text-webpack-plugin": "^1.0.1",
    "webpack": "1.13.3",
    "webpack-dev-server": "1.16.2",
    "postcss-loader": "^1.1.1",
    "style-loader": "^0.13.1",
    "json-loader": "^0.5.4",
    "file-loader": "^0.9.0",
    "css-loader": "^0.25.0",
    "url-loader": "^0.5.7",
    "temporary": "^0.0.8",
    "proxy-middleware": "0.15.0",
    "url": "0.11.0"
  },
  "babel": {
    "presets": ["es2015", "stage-0"],
    "plugins": ["inferno", "transform-async-to-generator", "transform-object-rest-spread", "transform-class-properties", "transform-runtime"]
  },
  "author": "Samuel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/samuelngs/weave/issues"
  },
  "homepage": "https://github.com/samuelngs/weave#readme"
}
